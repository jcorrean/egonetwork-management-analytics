@CONFERENCE{Kerr2008484,
author={Kerr, D. and Houghton, L.},
title={Feral systems: The likely effects on business analytics functions in an Enterprise Resource Planning System environment},
journal={ACIS 2008 Proceedings - 19th Australasian Conference on Information Systems},
