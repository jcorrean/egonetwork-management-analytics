@CONFERENCE{Shanks2012,
author={Shanks, G. and Bekmamedova, N. and Willcocks, L.},
title={Business analytics: Enabling strategic alignment and organisational transformation},
journal={ECIS 2012 - Proceedings of the 20th European Conference on Information Systems},
year={2012},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-84905727988&partnerID=40&md5=81ffb92b48d1796351a12b3f74e9617e},
affiliation={University of Melbourne, Carlton, Melbourne, 3010, Australia; London School of Economics and Political Science, London, United Kingdom},
abstract={Business analytics systems can potentially create value and provide competitive advantage to organisations. We argue that business analytics systems can also enable alignment between business strategy and IT strategy and support organisational transformation. We explain how a data warehouse, standardised metrics and global reporting supported the strategic transformation of a diversified and multi-layered international mining company to a unified and integrated global organisation. We identify a number of factors critical to the success of the business analytics initiative and to strategic alignment and organisational transformation, including: the early definition of globally standard metrics and dimensions; a high quality technology infrastructure and high quality data; sustained senior management support together with strong leadership within the business analytics project; effective governance structures that support change management and continual renewal of business analytics capabilities; and hybrid people with a mix of technical, business and strong communication skills. A number of implications of the study are discussed and suggestions for future work are provided.},
author_keywords={Business analytics;  Data warehousing;  Global reporting;  Strategic alignment},
keywords={Competition;  Data warehouses;  Information management;  Information systems;  Outsourcing, Business analytics;  Competitive advantage;  Global reporting;  Governance structures;  Organisational transformations;  Senior management support;  Strategic alignment;  Technology infrastructure, Metadata},
