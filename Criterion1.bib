@BOOK{Ramakrishna2008222,
author={Ramakrishna, H. and Sarkar, A. and Bachani, J.},
title={Business analytics success: A conceptual framework and an application to virtual organizing},
journal={Connectivity and Knowledge Management in Virtual Organizations: Networking and Developing Interactive Communications},
year={2008},
pages={222-254},
doi={10.4018/978-1-60566-070-7.ch013},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-84901514191&doi=10.4018%2f978-1-60566-070-7.ch013&partnerID=40&md5=b4d7b11704d8e621b26d1f3cb15f2e63},
