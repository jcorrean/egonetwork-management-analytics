@CONFERENCE{Eitle20191013,
author={Eitle, V. and Buxmann, P.},
title={Business analytics for sales pipeline management in the software industry: A machine learning perspective},
journal={Proceedings of the Annual Hawaii International Conference on System Sciences},
year={2019},
volume={2019-January},
pages={1013-1022},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85083449421&partnerID=40&md5=73ca53c53af251471916b0b8e9b610ff},
affiliation={Technische Universität Darmstadt},
abstract={This study proposes a model designed to help sales representatives in the software industry to manage the complex sales pipeline. By integrating business analytics in the form of machine learning into lead and opportunity management, data-driven qualification support reduces the high degree of arbitrariness caused by professional expertise and experiences. Through the case study of a software provider, we developed an artifact consisting of three models to map the end-to-end sales pipeline process using real business data from the company's CRM system. The results show a superiority of the CatBoost and Random Forest algorithm over other supervised classifiers such as Support Vector Machine, XGBoost, and Decision Tree as the baseline. The study also reveals that the probability of either winning or losing a sales deal in the early lead stage is more difficult to predict than analyzing the lead and opportunity phases separately. Furthermore, an explanation functionality for individual predictions is provided. © 2019 IEEE Computer Society. All rights reserved.},
keywords={Advanced Analytics;  Decision trees;  Pipelines;  Sales;  Software engineering;  Support vector machines;  Turing machines, Individual prediction;  Opportunity management;  Pipeline management;  Professional expertise;  Random forest algorithm;  Real business data;  Sales representatives;  Supervised classifiers, Learning systems},
